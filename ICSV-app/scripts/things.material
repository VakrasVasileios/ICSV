font IcsvFont
{
    type         truetype
    source       OpenSans-BoldItalic.ttf
    size         16
    resolution   72
}

material CubeMat
{
	technique
	{
		pass
		{
			ambient 1 0.4 0.4
			diffuse 1 0.7 0
			emissive 0.3 0.1 0
		}
  }
}

material BillboardMat
{
	receive_shadows off
	technique
	{
		pass
		{
			lighting off
      scene_blend alpha_blend

      ambient 0 0 0 1
			diffuse 0 0 0 1
 			emissive 0 0 0 1
		}
  }
}